<div class="container-fluid">
    <div class="row p-2">
        <div class="col-6 p-3">
            <div class="page-pretitle">
                Overview
            </div>
            <h2 class="page-title">
                {{this.title}}
            </h2>
        </div>
        <div class="form-group pt-2 col-6">
            <label for="formGroupExampleInput">Search Post</label>
            <div class="form-outline">
                <input type="search" id="searchUser" class="form-control"
                    placeholder="Search by first name, last name, full name & email" />
            </div>
        </div>
    </div>
    <div class="filterUser row justify-content-start" id="userList">
        {{> user/filter}}
    </div>
</div>