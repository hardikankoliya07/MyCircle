<div class="container-fluid">
    <div class="col p-3">
        <div class="page-pretitle">
            Overview
        </div>
        <h2 class="page-title">
            {{this.title}}
        </h2>
    </div>
    <div class="row justify-content-start">
        {{#if data}}
        {{#each data}}
        <div class="col-md-3 mt-2 mb-2">
            {{> post/index}}
        </div>
        {{/each}}
        {{else}}
        <div style="text-align: center;">
            <h1 class="p-5">
                NO POST FOUND
            </h1>
        </div>
        {{/if}}
    </div>
</div>